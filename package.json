{
  "name": "MAIN_FIRE_DOMSIN_WORKSPACE",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "genx-cli": "genx-cli/cli.js"
  },
  "scripts": {
    "sync-logs": "python sync_github_logs.py",
    "dev": "concurrently \"pnpm:client\" \"pnpm:server\" \"pnpm:python:dev\"",
    "client": "vite --host 0.0.0.0 --port 5173",
    "server": "tsx --watch --env-file=.env services/server/index.ts",
    "python:dev": "uvicorn api.main:app --host 0.0.0.0 --port 8000 --reload",
    "start": "concurrently \"tsx --env-file=.env services/server/index.ts\" \"uvicorn api.main:app --host 0.0.0.0 --port 8000\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "test": "vitest"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.0.0",
    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "concurrently": "^9.0.0",
    "cors": "^2.8.6",
    "drizzle-orm": "^0.45.0",
    "embla-carousel-react": "^8.0.0",
    "express": "^5.2.1",
    "firebase-tools": "^15.0.0",
    "lucide-react": "^0.500.0",
    "nanoid": "^5.1.6",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.70.0",
    "react-router-dom": "^6.30.0",
    "recharts": "^3.1.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.5.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.31.8",
    "esbuild": "^0.27.2",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.5.0",
    "supertest": "^7.1.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.0",
    "vitest": "^3.2.0",
    "ws": "^8.18.0"
  }
}
